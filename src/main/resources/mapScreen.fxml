<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox maxHeight="1440.0" maxWidth="2560.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.app.MapScreenController">
   <children>
      <StackPane fx:id="mapPane" minHeight="1080.0" prefWidth="1200.0" />
      <VBox fx:id="filterBox" minHeight="1080.0" minWidth="200.0" prefHeight="1080.0" prefWidth="200.0" spacing="5.0">
         <children>
            <Label fx:id="filterBoxLabel" prefHeight="103.0" prefWidth="156.0" text="Filter">
               <font>
                  <Font size="70.0" />
               </font>
            </Label>
            <Label fx:id="languageProgramsLabel" text="Language programs" textFill="#0f6ad6">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <CheckBox fx:id="spanishCheckbox" mnemonicParsing="false" text="has Spanish">
               <font>
                  <Font size="14.0" />
               </font></CheckBox>
            <CheckBox fx:id="frenchCheckbox" mnemonicParsing="false" text="has French Immerison">
               <font>
                  <Font size="14.0" />
               </font></CheckBox>
            <Label fx:id="schoolBoardLabel" text="School board" textFill="#0f6ad6">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <CheckBox fx:id="publicCheckbox" mnemonicParsing="false" text="Public">
               <font>
                  <Font size="14.0" />
               </font></CheckBox>
            <CheckBox fx:id="catholicCheckbox" mnemonicParsing="false" text="Catholic">
               <font>
                  <Font size="14.0" />
               </font></CheckBox>
            <Button fx:id="mapApplyButton" mnemonicParsing="false" onMouseClicked="#onMapApplyButtonClick" prefHeight="28.0" prefWidth="202.0" text="Apply">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Label fx:id="appliedFiltersLabel" text="Applied Filters: " textFill="#0f6ad6">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
            <Label fx:id="resultsReturnedLabel" text="n returned: ">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</HBox>
